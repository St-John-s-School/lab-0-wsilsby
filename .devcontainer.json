{
  "customizations":{
    "vscode": {
       "extensions": ["vscjava.vscode-java-pack", "formulahendry.code-runner"],
       "settings":{
        "files.exclude": {
          "**/.git": true,
          "**/.svn": true,
          "**/.hg": true,
          "**/CVS": true,
          "**/.DS_Store": true,
          "**/Thumbs.db": true,
          "**/*.crswap": true,
          "**/*.json": true
        }
      }
     },    
    
    "codespaces": {
      "openFiles":["README.md", "Runner.java"]
    }    
  }
}
